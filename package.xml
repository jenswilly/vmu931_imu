	<author email="jens@jwrobotics.com">Jens Willy Johannsen</author>

	<maintainer email="jmnavarrete@robotnik.es">Juan Manuel Navarrete</maintainer>
	<maintainer email="rnavarro@robotnik.es">Rom√°n Navarro</maintainer>

	<license>MIT</license>


